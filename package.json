{
  "name": "log-whisperer",
  "displayName": "log-whisperer",
  "description": "A VS Code extension for viewing log files in a structured way",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "media/icon.svg",
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "log-whisperer-explorer",
          "title": "Log Whisperer",
          "icon": "media/icon-light.svg"
        }
      ]
    },
    "views": {
      "log-whisperer-explorer": [
        {
          "icon": "media/icon-light.svg",
          "id": "logWhispererView",
          "name": "Log Explorer",
          "type": "webview"
        }
      ]
    },
    "commands": [
      {
        "command": "log-whisperer.selectLogFolder",
        "title": "Select Log Folder",
        "category": "Log Whisperer",
        "icon": "$(folder-opened)"
      },
      {
        "command": "log-whisperer.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "log-whisperer.parseLog",
        "title": "Parse Log File",
        "category": "Log Whisperer",
        "icon": {
          "light": "media/icon-light.svg",
          "dark": "media/icon-dark.svg"
        }
      },
      {
        "command": "log-whisperer.viewLog",
        "title": "View Log File",
        "category": "Log Whisperer"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "pnpm run check-types && pnpm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "pnpm run check-types && pnpm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.99.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "esbuild": "^0.25.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.8.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}